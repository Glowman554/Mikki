{
  "name": "mikki",
  "version": "0.0.1",
  "main": "src/electron.cjs",
  "author": "Glowman554 <glowman434@gmail.com>",
  "description": "Mikki ist ein Wiki, erstellt um die Assemblerprogrammierung im Schulunterricht zu dokumentieren. ",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "start": "svelte-kit start",
    "electron": "concurrently --kill-others \"svelte-kit dev\" \"electron src/electron.cjs\"",
    "electron:package": "npm run build && electron-builder --config electron-builder.config.json",
    "tauri": "concurrently --kill-others \"svelte-kit dev\" \"tauri dev -- --debug\"",
    "tauri:package": "npm run build && tauri build"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "next",
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "@tauri-apps/cli": "^1.0.0-beta.10",
    "concurrently": "^6.0.0",
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5",
    "electron-reloader": "^1.2.0",
    "svelte": "^3.29.0",
    "svelte-preprocess": "^4.0.0",
    "vite": "^2.1.0"
  },
  "type": "module",
  "dependencies": {
    "electron-serve": "^1.1.0",
    "electron-window-state": "^5.0.3",
    "svelte-markdown": "^0.2.2"
  }
}
