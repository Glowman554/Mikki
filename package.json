    "electron-builder": "^22.10.5",
    "electron-reloader": "^1.2.0",
    "svelte": "^3.29.0",
    "svelte-preprocess": "^4.0.0",
    "vite": "^2.1.0"
  },
  "type": "module",
  "dependencies": {
	"electron-serve": "^1.1.0",
    "electron-window-state": "^5.0.3",
    "svelte-markdown": "^0.2.2"
  }
}
