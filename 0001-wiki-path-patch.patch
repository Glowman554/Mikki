diff --git a/src/app.html b/src/app.html
index a3de035..dae6bb5 100644
--- a/src/app.html
+++ b/src/app.html
@@ -1,8 +1,8 @@
 <!DOCTYPE html>
 <html lang="en">
 	<head>
-		<script src="/commit.js"></script>
-		<script src="/mermaid.min.js"></script>
+		<script src="/wiki/commit.js"></script>
